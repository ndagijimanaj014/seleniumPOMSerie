<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">




<suite name="TestNG Regression Suite" verbose="3" parallel="tests" thread-count="3" >


 <test name="OpenCartLoginTest">
  <parameter name= "browser" value="chrome"/>
  <parameter name="url" value="https://naveenautomationlabs.com/opencart/index.php?route=account/success"/>
  <classes>
   <class name="com.qa.opencart.tests.LoginPageTest" />
  </classes>
 </test>
 <test name="OpenCartAccountPageTest">
  <parameter name= "browser" value="edge"/>
  <parameter name="url" value="https://naveenautomationlabs.com/opencart/index.php?route=account/success"/>
  <classes>
   <class name="com.qa.opencart.tests.AccountPageTest" />
  </classes>
 </test>


</suite>